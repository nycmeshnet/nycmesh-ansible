# Managed by ansible
# Target DNS server
dns_rfc2136_server = {{ certbot_main_auth_dns_server | default('199.170.132.47') }}
# Target DNS port
dns_rfc2136_port = 53
# TSIG key name
dns_rfc2136_name = {{ certbot_tsig_key_name | default(certbot_get_cert_for_domain) }}
# TSIG key secret
dns_rfc2136_secret = {{ certbot_tsig_key }}
# TSIG key algorithm
dns_rfc2136_algorithm = HMAC-SHA512
